{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },

  "parser": "babel-eslint",

  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module"
  },

  "extends": ["eslint:recommended", "plugin:prettier/recommended", "prettier"],

  "plugins": ["prettier"],

  "rules": {
    "no-console": "off"
  }
}
