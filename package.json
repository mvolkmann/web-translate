{
  "name": "web-translate",
  "description": "supports language translation in web applications",
  "author": "R. Mark Volkmann <r.mark.volkmann@gmail.com> (http://mvolkmann.github.io)",
  "version": "1.2.1",
  "keywords": [
    "language",
    "translation",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/web-translate.git"
  },
  "main": "src/index.js",
  "bin": {
    "generate-translations": "lib/generate-translations.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "clean": "rm -rf lib",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "npm run build",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "jest --testPathPattern=src/"
  },
  "dependencies": {
    "@babel/core": "^7.17.0",
    "@babel/runtime": "^7.17.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "babel-eslint": "^10.1.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esm": "^3.2.25",
    "fetch-mock": "^9.11.0",
    "jest": "^27.5.0",
    "node-fetch": "^3.2.0",
    "prettier": "^2.5.1"
  }
}
