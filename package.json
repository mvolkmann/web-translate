{
  "name": "web-translate",
  "description": "supports language translation in web applications",
  "author": "R. Mark Volkmann <r.mark.volkmann@gmail.com> (http://mvolkmann.github.io)",
  "version": "0.7.5",
  "keywords": [
    "language",
    "translation",
    "web"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvolkmann/web-translate.git"
  },
  "main": "lib/index.js",
  "bin": {
    "generate-translations": "lib/generate-translations.js"
  },
  "scripts": {
    "build": "babel src -d lib",
    "clean": "rm -rf lib",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint --quiet src --ext .js",
    "prepublish": "npm run build",
    "reinstall": "rm -rf node_modules package-lock.json && npm install",
    "test": "jest --testPathPattern=src/"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "esm": "^3.2.25",
    "fetch-mock": "^7.3.3",
    "jest": "^24.8.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2"
  }
}
